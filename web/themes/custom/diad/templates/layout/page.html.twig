<div class="site-wrapper" id="site-wrapper">
  {% if page.top %}
    <section class="wrapper-root wrapper-top" id="wrapper-top">
      <div class="l-top">
        <div class="l-site-width l-top-inner">
          {{ page.top }}
        </div>
      </div>
    </section>
  {% endif %}

  <header class="wrapper-root wrapper-header" id="wrapper-header" role="banner">
    <div class="l-header">
      <div class="l-site-width l-header-inner">
        <div class="r-header r-header-main">
