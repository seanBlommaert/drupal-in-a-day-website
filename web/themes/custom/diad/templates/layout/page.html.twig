                <path fill="#fff" class="logo__clock" d="M15 41.1h-2.8c-.3 0-.6.2-.6.6v2.8c0 .3.3.6.6.6H15c.3 0 .6-.3.6-.6v-2.8c0-.3-.3-.6-.6-.6m11.2 10.6h-2.8c-.3 0-.6.2-.6.6v2.8c0 .3.3.6.6.6h2.8c.3 0 .6-.3.6-.6v-2.8c0-.3-.3-.6-.6-.6M35.7 41h-9V30.7l-2-2.9-2 2.8v14.3h12.8l2.3-2-2.1-1.9z"/>
              </svg>
            </a>
          <button class="btn btn__menu">
            <span class="btn__menu--span btn__menu--collapsed">
              {{ "Menu"|t }}
              <svg class="icon icon__menu">
                <use xlink:href="{{ theme_path }}/images/svgsprite.svg#icon-menu"></use>
              </svg>
            </span>
            <span class="btn__menu--span btn__menu--expanded">
              {{ "Close"|t }}
              <svg class="icon icon__close">
                <use xlink:href="{{ theme_path }}/images/svgsprite.svg#icon-close"></use>
              </svg>
            </span>
          </button>
        </div>
        <div class="r-header r-header-navigation">
          {{ page.navigation }}
        </div>
      </div>
    </div>
  </header>
  <a class="visually-hidden" id="main-content" tabindex="-1"></a>
  <main class="wrapper-root wrapper-main" id="wrapper-main" role="main">

    {% if page.breadcrumb %}
      <div class="l-breadcrumb">
        <div class="l-site-width l-breadcrumb-inner">
          {{ page.breadcrumb }}
        </div>
      </div>
    {% endif %}

    {% if page.messages %}
      {{ page.messages }}
    {% endif %}

    {% if page.above_content %}
      <div class="l-above-content">
        <div class="l-site-width l-above-content-inner">
          {{ page.above_content }}
        </div>
      </div>
    {% endif %}

    {% if page.content_top %}
      <div class="l-content-top">
        <div class="l-site-width l-content-top-inner">
          {{ page.content_top }}
        </div>
      </div>
    {% endif %}

    {% if page.content %}
      <div class="l-content">
          <div class="r-content r-content-main" role="complementary">
            {{ page.content }}
          </div>
      </div>
    {% endif %}

    {% if page.content_bottom %}
      <div class="l-content-bottom">
        <div class="l-site-width l-content-bottom-inner">
          {{ page.content_bottom }}
        </div>
      </div>
    {% endif %}

    {% if page.below_content %}
      <div class="l-below-content">
        <div class="l-site-width l-below-content-inner">
          {{ page.below_content }}
        </div>
      </div>
    {% endif %}

  </main>

  <footer class="wrapper-root wrapper-footer" id="wrapper-footer" role="contentinfo">
    <div class="l-site-width wrapper-footer-inner">
    {% if page.footer_first %}
      <div class="l-footer l-footer-first">
        {{ page.footer_first }}
        <svg class="bg-footer">
          <use xlink:href="{{ theme_path }}/images/svgsprite.svg#bg-footer"></use>
        </svg>
      </div>
    {% endif %}
    {% if page.footer_second %}
      <div class="l-footer l-footer-second">
        {{ page.footer_second }}
      </div>
    {% endif %}
    {% if page.footer_third %}
      <div class="l-footer l-footer-third">
        {{ page.footer_third }}

      </div>
    {% endif %}
    </div>
  </footer>
</div>
