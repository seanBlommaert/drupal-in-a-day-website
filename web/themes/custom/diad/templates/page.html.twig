{# Page start #}
<div class="site-wrapper" id="site-wrapper">
  {% if page.top %}
    <section class="wrapper-root wrapper-top" id="wrapper-top">
      <div class="l-color l-top">
        <div class="l-site-width l-top-inner">
          {{ page.top }}
        </div>
      </div>
    </section>
  {% endif %}

